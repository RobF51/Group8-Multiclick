<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#3A0CA3"
    android:backgroundTint="#3A0CA3"
    android:foregroundTint="#6C2121"
    tools:context=".MainComprador">


    <TextView
        android:id="@+id/NombreCategFix"
        android:layout_width="224dp"
        android:layout_height="61dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="92dp"
        android:fontFamily="sans-serif-light"
        android:text="CATEGORIAS"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/viewcategorias"
        android:layout_width="0dp"
        android:layout_height="593dp"
        android:layout_marginTop="84dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/NombreCategFix" />








</androidx.constraintlayout.widget.ConstraintLayout>